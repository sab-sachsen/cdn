{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["import", "@typescript-eslint"],
  "root": true,
  "rules": {
    "import/first": 0
  },
  "settings": {
    "import/resolver": {
      "typescript": true,
      "node": true
    }
  }
}
