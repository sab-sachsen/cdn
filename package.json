    "etag": "1.8.1",
    "express": "4.18.2",
    "gunzip-maybe": "1.4.2",
    "highlight.js": "9.18.5",
    "isomorphic-fetch": "3.0.0",
    "jsesc": "2.5.2",
    "lru-cache": "5.1.1",
    "mime": "2.6.0",
    "morgan": "1.10.0",
    "ndjson": "1.5.0",
    "pretty-bytes": "5.6.0",
    "prop-types": "15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-icons": "3.11.0",
    "semver": "5.7.1",
    "sort-by": "1.2.0",
