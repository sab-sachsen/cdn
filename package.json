{
  "name": "@sab/cdn",
  "private": true,
  "description": "A CDN service for frontend artifacts within the SAB infrastructure",
  "license": "AGPL-3.0-or-later",
  "main": "dist/index.js",
  "files": [
    ".env.example",
    "dist",
    "Dockerfile",
    "docker-compose.yml"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "ts-node --transpileOnly --logError ./esbuild.config.ts",
    "start": "nodemon",
    "prestart:docker": "npm run build",
    "start:docker": "docker compose up"
  },
  "dependencies": {
    "@types/gunzip-maybe": "1.4.0",
    "cors": "2.8.5",
    "etag": "1.8.1",
    "express": "4.18.2",
    "gunzip-maybe": "1.4.2",
    "lru-cache": "8.0.4",
    "mime": "3.0.0",
    "morgan": "1.10.0",
    "semver": "7.3.8",
    "sri-toolbox": "0.2.0",
    "tar-stream": "3.0.0",
    "validate-npm-package-name": "5.0.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/etag": "1.8.1",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.0",
    "@types/morgan": "1.9.4",
    "@types/node": "18.15.3",
    "@types/supertest": "2.0.12",
    "@types/tar-stream": "2.2.2",
    "@types/validate-npm-package-name": "4.0.0",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "dotenv": "16.0.3",
    "esbuild": "0.17.12",
    "esbuild-copy-static-files": "0.1.0",
    "esbuild-plugin-clean": "1.0.1",
    "eslint": "8.36.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "git-rev-sync": "3.0.2",
    "jest": "29.5.0",
    "nodemon": "2.0.21",
    "supertest": "6.3.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "engines": {
    "node": "18.15.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/src/__mocks__/style.mock.ts",
      "\\.png$": "<rootDir>/src/__mocks__/image.mock.ts",
      "getStats": "<rootDir>/src/__mocks__/get-stats.mock.ts",
      "utils\\/npm": "<rootDir>/src/__mocks__/npm.mock.ts"
    },
    "preset": "ts-jest",
    "testMatch": [
      "**/*.test.ts"
    ],
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    }
  },
  "version": "0.0.4"
}
